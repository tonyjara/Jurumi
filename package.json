  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true next build ",
    "build": "next build",
    "build:staging": "env-cmd -f .env.staging pnpm build",
    "dev": "concurrently -n \"[lint,next]\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"tsc --noEmit --incremental --watch\" \"NODE_ENV=development && next dev \"",
    "dev:staging": "env-cmd -f .env.staging  next dev",
    "docker-staging": "docker compose -f compose-staging.yml up --build --force-recreate",
    "lint": "next lint",
    "migrate:prod:DANGER": "env-cmd -f .env.prod npx prisma migrate dev ",
    "migrate:staging": "env-cmd -f .env.staging npx prisma migrate dev ",
    "postinstall": "prisma generate && prisma migrate deploy",
    "prod": "env-cmd -f .env.prod  next dev",
    "prod:studio": "env-cmd -f .env.prod npx prisma studio",
    "staging": "env-cmd -f .env.staging concurrently -n \"[lint,next]\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"tsc --noEmit --incremental --watch\" \"NODE_ENV=development && next dev \"",
    "start": "next start",
    "start:prod": "env-cmd -f .env.prod next dev",
    "start:staging": "env-cmd -f .env.staging next dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "ts-lint": "tsc --noEmit --incremental --watch"
  },
