  "private": true,
  "scripts": {
    "staging": "env-cmd -f .env.staging concurrently -n \"[lint,next]\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"tsc --noEmit --incremental --watch\" \"NODE_ENV=development && next dev \"",
    "build:staging": "env-cmd -f .env.staging yarn build",
    "start:staging": "env-cmd -f .env.staging next start",
    "migrate:staging": "env-cmd -f .env.staging npx prisma migrate dev ",
    "build": "NODE_ENV=production && next build",
    "dev": "concurrently -n \"[lint,next]\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"tsc --noEmit --incremental --watch\" \"NODE_ENV=development && next dev \"",
    "start": "NODE_ENV=production && next start",
    "lint": "next lint",
    "test": "jest --watch",
    "postinstall": "prisma generate",
    "ts-lint": "tsc --noEmit --incremental --watch",
    "docker-staging": "docker compose -f compose-staging.yml up --build --force-recreate"
  },
